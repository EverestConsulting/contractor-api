<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.contractor.model.entity.UserRole" table="user_role" schema="public" catalog="contractor">
        <id name="userRoleId">
            <column name="user_role_id" sql-type="integer"/>
            <generator class="identity"/>
        </id>
        <property name="userRoleName">
            <column name="user_role_name" sql-type="varchar(20)" length="20"/>
        </property>
    </class>
</hibernate-mapping>